{
  "name": "rohan",
  "version": "1.0.0",
  "description": "OpenAPI Test Generator for k6 - LLM-powered test generation",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "rohan": "./dist/bin/rohan.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/bin/rohan.js",
    "rohan": "node dist/bin/rohan.js",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src/**/*.ts",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "openapi",
    "testing",
    "k6",
    "llm",
    "api-testing",
    "test-generation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ai": "^4.3.16",
    "@ai-sdk/groq": "^1.2.9",
    "@ai-sdk/openai": "^1.3.22",
    "@ai-sdk/anthropic": "^1.2.12",
    "commander": "^13.1.0",
    "chalk": "^5.4.1",
    "ora": "^8.2.0",
    "p-limit": "^6.2.0",
    "dotenv": "^16.5.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4",
    "rimraf": "^6.0.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
